{
  "compilerOptions": {
      
                                    
    "lib": ["dom","dom.iterable","esnext"],                                        
     "moduleResolution": "node", 
     "jsx":"react-jsx",                      
     "resolveJsonModule": true,  
     "strict":true,                      
     "allowJs": true,                                  
     "noEmit": true,                                   
     "isolatedModules": true,                          
     "allowSyntheticDefaultImports": true,             
     "esModuleInterop": true,                             
     "forceConsistentCasingInFileNames": true,            
     "skipLibCheck": true                                 
  },
  "include": ["src"]
}
